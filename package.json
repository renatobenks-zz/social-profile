{
  "name": "CodeRockrApplication",
  "version": "0.1.0",
  "private": false,
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "codecov": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.3"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "helmet": "^3.5.0",
    "hpp": "^0.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.67.2",
    "serve-favicon": "^2.4.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_ENV=development node ./index.js",
    "production": "./node_modules/.bin/cross-env NODE_ENV=production node ./index.js",
    "build": "./node_modules/.bin/webpack -p --config ./webpack.config.prod.js",
    "test": "./node_modules/.bin/jest --colors -u",
    "test:report": "npm test && ./node_modules/.bin/codecov",
    "test:watch": "npm test -- --watch",
    "clean": "rm -rf build/ assets.json"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".*": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": "\\.test\\.jsx?$"
  }
}
